<h1>Photos#update</h1>
<%= form_for(@photo) do |f| %>
  <div class="field">
    <%= f.label :title%><br>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :picture%> url <br>
    <%= f.text_field :picture %>
  </div>
  <div class="actions"><%= f.submit %></div>
<% end %>
